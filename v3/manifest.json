{
  "name": "Extension Manager with Profiles",
  "description": "__MSG_description__",
  "default_locale": "en",
  "version": "0.2.3",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "management"
  ],
  "action": {
    "default_popup": "/data/panel/index.html"
  },
